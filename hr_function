#include<bits/stdc++.h>
using namespace std;
int max(int a,int b ,int c ,int d){
    cin>>a>>b>>c>>d;
    if((a>b)&&(a>c)&&(a>d)){
        cout<<a;
        return a;
    }
    else if((b>a)&&(b>c)&&(b>d)){
        cout<<b;
        return b;
    }
    else if ((c>a)&&(c>b)&&(c>d)){
        cout<<c;
        return c;
    }
    else {
    cout<<d;
    return d;
    }
}

int main(){
    int Max,a,b,c,d;
    Max=max(a,b,c,d);
    
}
